{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "gen liblua",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/regenerator.exe",
            "args": [
                "${workspaceFolder}/lua/d_liblua.lua",
                "${workspaceFolder}/submodules/lua",
                "${workspaceFolder}/source/liblua",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "PATH",
                    "value": "C:/Program Files/LLVM/bin"
                }
            ],
            "externalConsole": false,
            "visualizerFile": "${workspaceFolder}/.vscode/d.natvis"
        },
        {
            "name": "gen libclang",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/regenerator.exe",
            "args": [
                "${workspaceFolder}/lua/d_libclang.lua",
                "C:/Program Files/LLVM/include",
                "${workspaceFolder}/source/libclang",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "PATH",
                    "value": "C:/Program Files/LLVM/bin"
                }
            ],
            "externalConsole": false,
            "visualizerFile": "${workspaceFolder}/.vscode/d.natvis"
        },
        {
            "name": "gen d3d11",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/regenerator.exe",
            "args": [
                "${workspaceFolder}/lua/d_d3d11.lua",
                "C:/Program Files (x86)/Windows Kits/10/Include/10.0.17763.0",
                "${workspaceFolder}/source/d3d11",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "PATH",
                    "value": "C:/Program Files/LLVM/bin"
                }
            ],
            "externalConsole": false,
            "visualizerFile": "${workspaceFolder}/.vscode/d.natvis"
        },
        {
            "name": "dclangen imgui",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/regenerator.exe",
            "args": [
                "--outdir",
                "${workspaceFolder}/source/imgui",
                "-E",
                "-H",
                "${workspaceFolder}/submodules/imgui/imgui.h",
                "-D",
                "IMGUI_API=__declspec(dllexport)",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "PATH",
                    "value": "C:/Program Files/LLVM/bin"
                }
            ],
            "externalConsole": false,
            "visualizerFile": "${workspaceFolder}/.vscode/d.natvis"
        },
        {
            "name": "clang_sample",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/Debug/bin/clang_sample.exe",
            "args": [
                // "${workspaceFolder}/main.cpp",
                "C:/Program Files/LLVM/include/clang-c/Index.h",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "PATH",
                    "value": "C:/Program Files/LLVM/bin"
                }
            ],
            "externalConsole": false
        },
        {
            "name": "cpptypeinfo libclang",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/submodules/cpptypeinfo/cpptypeinfo/cli.py",
            "args": [
                "gen",
                "dlang",
                "source/libclang",
                "-I",
                "C:/Program Files/LLVM/include",
                "--header",
                "C:/Program Files/LLVM/include/clang-c/Index.h",
                "--header",
                "C:/Program Files/LLVM/include/clang-c/CXString.h",
            ],
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/submodules/cpptypeinfo",
            }
        },
    ]
}